게시판 테이블
CREATE TABLE "FILESPRING" 
   (	"NUM" NUMBER NOT NULL ENABLE, 
	"WRITER" VARCHAR2(30), 
	"EMAIL" VARCHAR2(30), 
	"SUBJECT" VARCHAR2(50), 
	"PASSWD" VARCHAR2(30), 
	"REG_DATE" VARCHAR2(10), 
	"READCOUNT" NUMBER, 
	"CONTENT" VARCHAR2(4000), 
	"IP" VARCHAR2(20), 
	"FILENAME" VARCHAR2(4000), 
	"RE_STEP" NUMBER(38,0), 
	"RE_LEVEL" NUMBER(38,0)
   )
게시판 시퀀스
create sequence file_seq
